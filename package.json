{
  "name": "tokusolutions",
  "version": "1.0.0",
  "type": "module",
  "description": "Web viewer for translated toy manuals",
  "scripts": {
    "serve": "python -m http.server 8000",
    "lint": "prettier --check 'manuals/**/translations.json' 'web/**/*.{js,json,html,css}'",
    "format": "prettier --write 'manuals/**/translations.json' 'web/**/*.{js,json,html,css}'"
  },
  "devDependencies": {
    "prettier": "^3.7.4"
  }
}
